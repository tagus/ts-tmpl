{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "baseUrl": "./",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "./*"
      ],
    },
    "useUnknownInCatchVariables": false,
    "resolveJsonModule": true,
    "jsx": "react",
  },
  "include": [
    "./**/*.tsx",
    "./**/*.ts",
  ],
  "exclude": [
    "node_modules",
  ],
}
